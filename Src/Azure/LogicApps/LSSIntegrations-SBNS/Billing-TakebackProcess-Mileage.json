                            "queries": {
                                "$select": "statuscode"
                            }
                        },
                        "runAfter": {
                            "Update_mileageentry": [
                                "SUCCEEDED"
                            ]
                        }
                    },
                    "Update_salesorder": {
                        "type": "ApiConnection",
                        "inputs": {
                            "host": {
                                "connection": {
                                    "referenceName": "commondataservice-1"
                                }
                            },
                            "method": "patch",
                            "body": {
                                "statuscode": "@if(and(or(equals(trim(body('Parse_JSON')?['RecordType']), 'Original'),equals(trim(body('Parse_JSON')?['RecordType']), 'OriginalFee')),equals(body('Get_a_row_by_ID')?['statuscode'], 0)), 456010013, body('Get_a_row_by_ID')?['statuscode'])"
