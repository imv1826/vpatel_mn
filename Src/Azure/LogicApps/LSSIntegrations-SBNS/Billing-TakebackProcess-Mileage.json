                                        "type": "integer"
                                    },
                                    "pics_PortalStatus": {
                                        "type": "integer"
                                    },
                                    "pics_StartDate": {
                                        "type": "string"
                                    },
                                    "pics_VoucherNumber": {
                                        "type": "string"
                                    },
                                    "new_clientid": {
                                        "type": "string"
                                    },
                                    "new_paytoid": {
                                        "type": "string"
                                    },
                                    "new_budgetlineid": {
